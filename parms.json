{
    "version": "1.1",
    "general": {
        "nocolor": false,
        "prefix": [
            {
                "0": "${PWD}"
            },
            {
                "1": "${QASM_BASE}"
            },
            {
                "2": "0/source"
            },
            {
                "3": "0/object"
            },
            {
                "4": "1/macros"
            },
            {
                "5": "1/lib"
            },
            {
                "6": "1/commands"
            },
            {
                "7": "1/help"
            },
            {
                "8": "object",
                "volume": "0/merlin.2mg",
                "size": "800K",
                "format": "prodos",
                "fileformat": "2mg"
            }
        ]
    },
    "language": "qasm", 
    "asm": {
        "cpu": "M65816",
        "start_mx": 0,
        "listmode": "on",
        "casesen": true,
        "start_lst": false,
        "showmx": true,
        "allowduplicate": true,
        "trackrep": false,
        "merlinerrors": true,
        "m32vars": false,
        "allowA": true,
        "allowLocal": true,
        "allowColon": true,
        "repsep": "force", 
        "linebytes": 4,
        "line2bytes": 8
    },
    "linker": {},
    "format": {
        "tabs": "12,18,30,48"
    }
}